{
    "root": true,
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true,
        "browser": true,
        "mocha": true
    },
    "extends": ["eslint:recommended", "plugin:cypress/recommended"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
    },
    "plugins": [
        "cypress"
      ]
}
